<h3>Export</h3>
<%= link_to "Retrieve as XML", quotations_path(:format => 'xml',search: params[:search]) ,:target => "_blank"%><br/>
<%= link_to "Retrieve as JSON", quotations_path(:format => 'json',search: params[:search]) ,:target => "_blank"%><br/>

<h3>Import XML File</h3>
<%= form_tag({action: :upload}, multipart: true, :accept => 'text/xml') do %>
  <%= file_field_tag 'xml_file' %></br>
  <%= submit_tag 'Upload'%>
<% end %>
